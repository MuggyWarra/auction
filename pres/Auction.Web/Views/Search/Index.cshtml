@model Slot[]

@{
    ViewData["Title"] = "Результаты поиска";
}

<h1>Результаты поиска</h1>
@if (Model.Length == 0)
            {
                <p>Ничего не найдено</p>
            }
            else
            {
            <ul>
                @foreach (var slot in Model)
                 {
                    <li><a href="@Url.Action("Index","Slot",new {id= slot.Id })">@slot.Title</a></li>
                 }
            </ul>
            }
